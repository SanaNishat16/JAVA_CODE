
public class Comparison {

	public static void main(String[] args) {
		                           
		 String s1 = "hello";
		 String s2 = new String ("hello");
		 System.out.println(s1 == s2);

	String str1 = new String("Java");
	String str2 = str1;
	System.out.println(str1==str2);
	
	String str = "Sky";
	String str3 ="sky";
	System.out.println(str==str3);
	
	}

}
